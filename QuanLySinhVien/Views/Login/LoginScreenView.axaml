<Window
    x:Class="QuanLySinhVien.Views.Login.LoginScreenView"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:tittlebars="clr-namespace:CustomTitleBarTemplate.Views.CustomTitleBars"
    xmlns:vm="using:QuanLySinhVien.ViewModels.Login"
    Title="LoginScreenView"
    Width="900"
    Height="550"
    d:DesignHeight="550"
    d:DesignWidth="900"
    x:DataType="vm:LoginScreenViewModel"
    mc:Ignorable="d">
    <DockPanel>
        <tittlebars:WindowsTitleBar Height="25" />
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition />
                <ColumnDefinition />
            </Grid.ColumnDefinitions>

            <Panel Grid.Column="0" Background="Red" />
            <Panel Grid.Column="1" Background="Blue" />
        </Grid>
    </DockPanel>
</Window>
